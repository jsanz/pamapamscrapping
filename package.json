{
    "name": "pamapamscrapping",
    "version": "1.0.0",
    "description": "Small scrapper for Pam a Pam website to collect and analyze data",
    "main": "src/index.ts",
    "repository": "git@github.com:jsanz/pamapamscrapping.git",
    "author": "Jorge Sanz <jorge.sanz@elastic.co>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "clean": "rm -rf .cache dist",
        "eslint": "eslint",
        "ts-node": "ts-node",
        "build": "tsc",
        "dev": "DEBUG=pamapam ts-node src/index.ts",
        "dev:w": "DEBUG=pamapam nodemon",
        "prestart": "tsc",
        "start": "node dist/index.js"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.15",
        "@types/debug": "^4.1.5",
        "@types/geojson": "^7946.0.7",
        "@types/html-to-text": "^1.4.31",
        "@types/json2csv": "^4.5.0",
        "@types/puppeteer": "^2.0.0",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.45",
        "@types/yargs": "^13.0.3",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.1",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "@elastic/elasticsearch": "^7.5.0",
        "cheerio": "^1.0.0-rc.3",
        "debug": "^4.1.1",
        "html-to-text": "^5.1.1",
        "json2csv": "^4.5.4",
        "request": "^2.88.0",
        "request-promise": "^4.2.5",
        "yargs": "^15.0.2"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "ignore": [
            "src/**/*.spec.ts"
        ],
        "exec": "ts-node ./src/index.ts"
    }
}
